{
  "name": "ssr",
  "version": "1.0.0",
  "description": "Server Side Rendering",
  "main": "app.js",
  "scripts": {
    "start": "nodemon server.js",
    "start:prod": "env NODE_ENV=production node server.js",
    "debug": "ndb server.js",
    "debug:prod": "env NODE_ENV=production ndb server.js",
    "watch:js": "parcel watch ./public/js/index.js --out-dir ./public/js  --out-file bundle.js",
    "build:js": "parcel build ./public/js/index.js --out-dir ./public/js  --out-file bundle.js"
  },
  "author": "Harsh",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "mongoose": "^5.13.8",
    "morgan": "^1.10.0",
    "nodemailer": "^6.8.0",
    "pug": "^3.0.2",
    "slugify": "^1.6.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "ndb": "^1.1.5",
    "parcel-bundler": "^1.12.5"
  }
}
